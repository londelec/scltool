  <xs:simpleType name="AARQfieldsType">
    <xs:annotation>
      <xs:documentation>Fields included in [AARQ APDU]</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 Version">
            <xs:documentation>[Protocol Version] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 Called AP Title">
            <xs:documentation>[Called AP Title] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="2" caption="Bit 2 Called AE Qual">
            <xs:documentation>[Called AE Qualifier] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="3" caption="Bit 3 Called AP IvkID">
            <xs:documentation>[Called AP Invocation-identifier] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="4" caption="Bit 4 Called AE IvkID">
            <xs:documentation>[Called AE Invocation-identifier] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="5" caption="Bit 5 Calling AP Title">
            <xs:documentation>[Calling AP Title] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="6" caption="Bit 6 Calling AE Qual">
            <xs:documentation>[Calling AE Qualifier] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="7" caption="Bit 7 Calling AP IvkID">
            <xs:documentation>[Calling AP Invocation-identifier] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="8" caption="Bit 8 Calling AE IvkID">
            <xs:documentation>[Calling AE Invocation-identifier] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8650-1:1996]</xs:documentation>
          </le:bit>
          <le:bit value="15" caption="Bit 15 Direct Reference">
            <xs:documentation>[Direct Reference] in [Association-information] field Included(1) / Excluded(0) in outgoing [AARQ APDU] [ISO8825-1:2008]</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="AFlags">
    <xs:annotation>
      <xs:documentation>Association layer flags (bit encoded)</xs:documentation>
