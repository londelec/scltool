    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1024"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MLogFlags">
    <xs:annotation>
      <xs:documentation>Manufacturing Message Specification logger flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 Basic">
            <xs:documentation>Basic MMS information recording to logfile Enabled(1) / Disabled(0)</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 Verbose">
            <xs:documentation>All MMS data field value recording to logfile Enabled(1) / Disabled(0)</xs:documentation>
          </le:bit>
          <!--Debug flag is excluded from user XSD-->
          <!--le:bit value="7" caption="Bit 7 Debug">
            <xs:documentation>MMS layer debug recording to logfile Enabled(1) / Disabled(0)</xs:documentation>
          </le:bit-->
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="ByteType"/>
  </xs:simpleType>
  <xs:simpleType name="ScsmFlags">
    <xs:annotation>
      <xs:documentation>Specific Communication Service Mapping flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="4" caption="Bit 4 Alternate Access for Array">
            <xs:documentation>[AlternateAccess] is used for Full reference(1) / Array members only(0). Normally [AlternateAccess] is used only to address array members and their children. Array element itself is addressed with [VariableSpecification] [name]. Setting this bit will result in array element and its parents being addressed with [AlternateAccess]. [IEC9506-2:2003]</xs:documentation>
          </le:bit>
          <le:bit value="11" caption="Bit 11 Always Alternate Access">
            <xs:documentation>[AlternateAccess] is used for All elements(1) / Array members only(0). Normally [AlternateAccess] is used only to address array members and their children. Other elements are addressed with [VariableSpecification] [name]. Setting this bit will result in all elements being addressed with [AlternateAccess]. [IEC9506-2:2003]</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="BdyndsnameType">
    <xs:annotation>
      <xs:documentation>Name for dynamic datasets that are going to be linked to Buffered report blocks. IED name and preferred Logical Device where datasets have to be created can be included e.g. IEDLD0/dynb</xs:documentation>
    </xs:annotation>
    <xs:restriction base="DatasetNameType">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UdyndsnameType">
    <xs:annotation>
      <xs:documentation>Name for dynamic datasets that are going to be linked to Unbuffered report blocks. IED name and preferred Logical Device where datasets have to be created can be included e.g. LD0/dynu</xs:documentation>
    </xs:annotation>
    <xs:restriction base="DatasetNameType">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MaxdyndsRange">
    <xs:annotation>
      <xs:documentation>Maximal number of dynamic datasets that can be created. Datasets linked to Buffered and Unbuffered Report Blocks are accounted separately.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedByte">
   </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BOptFldsType">
    <xs:annotation>
      <xs:documentation>Optional Fields [OptFlds] for Buffered Report Control Block</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 EntryID">
            <xs:documentation>[entryID] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 BufOvfl">
            <xs:documentation>[buffer-overflow] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="2" caption="Bit 2 DataRef">
            <xs:documentation>[data-reference] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="3" caption="Bit 3 DatSet">
            <xs:documentation>[data-set-name] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="4" caption="Bit 4 ReasonCode">
            <xs:documentation>[reason-for-inclusion] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="5" caption="Bit 5 TimeOfEntry">
            <xs:documentation>[report-time-stamp] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="6" caption="Bit 6 SqNum">
            <xs:documentation>[sequence-number] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="15" caption="Bit 15 ConfRev">
            <xs:documentation>[conf-revision] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="UOptFldsType">
    <xs:annotation>
      <xs:documentation>Optional Fields [OptFlds] for Unbuffered Report Control Block</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 EntryID">
            <xs:documentation>[entryID] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 BufOvfl">
            <xs:documentation>[buffer-overflow] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="2" caption="Bit 2 DataRef">
            <xs:documentation>[data-reference] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="3" caption="Bit 3 DatSet">
            <xs:documentation>[data-set-name] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="4" caption="Bit 4 ReasonCode">
            <xs:documentation>[reason-for-inclusion] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="5" caption="Bit 5 TimeOfEntry">
            <xs:documentation>[report-time-stamp] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="6" caption="Bit 6 SqNum">
            <xs:documentation>[sequence-number] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
          <le:bit value="15" caption="Bit 15 ConfRev">
            <xs:documentation>[conf-revision] field in the Report Included(1) / Excluded(0) [IEC61850-8-1:2011]</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="orCatRange">
    <xs:annotation>
      <xs:documentation>Originator category value for outgoing commands [IEC61850-7-3:2011]</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="8"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="orIdentString">
    <xs:annotation>
      <xs:documentation>Originator identification value for outgoing commands [IEC61850-7-3:2011] (0...64 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:normalizedString">
      <xs:minLength value="1"/>
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="bufTimeRange">
    <xs:annotation>
      <xs:documentation>Interval in milliseconds for the buffering of internal notifications before Information Report is generated [IEC61850-7-2:2010] Value 0 disables buffering and Information Reports are generated instantly (0...4294967294)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="Timer32bitType">
      <xs:maxInclusive value="4294967294"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AcsiFlags">
    <xs:annotation>
      <xs:documentation>Abstract Communication Service Interface flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 Always Read Dir">
            <xs:documentation>In case IED supports only static datasets Read(1) / Don't read(0) Directory when station goes online</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 Read All Basic Types">
            <xs:documentation>Read Basic Types of All(1) / Elements non-existent in SCL(0) when station goes online with [GetDataDefinition]. If any new elements have been discovered in directory response that don't exist in the SCL tree, Basic Types of those will be requested. This setting doesn't always apply, please check XML manual for more information.</xs:documentation>
          </le:bit>
          <le:bit value="2" caption="Bit 1 Read linked Basic Types">
            <xs:documentation>Read(1) / Don't read(0) Basic Types of elements that are required for data acquisition (linked to DI/AI) with [GetDataDefinition]. This setting doesn't always apply, please check XML manual for more information.</xs:documentation>
          </le:bit>
          <le:bit value="4" caption="Bit 4 Ignore CmdTerm">
            <xs:documentation>[CommandTermination] after enhanced security command completion is Ignored(1) / Processed(0). This setting has to be used only if IED doesn't generate [CommandTermination] when Enhanced security Direct or SBO command is complete. If there is no termination and this setting is not used, the command will only be completed after Application timeout expiration.</xs:documentation>
          </le:bit>
          <le:bit value="5" caption="Bit 5 Use millisec accuracy">
            <xs:documentation>Millisecond accuracy of timetags received from IED is Used(1) / Ignored(0). Some IEDs report lower accuracy if the IED is not synchronized. Enabling this setting will result in rounding of milliseconds based on the received accuracy and the timetag may appear different than record in IED's internal event list.</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="RCBFlags">
    <xs:annotation>
      <xs:documentation>Report Contrl Block flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 Set PurgeBuf">
            <xs:documentation>Set(1) / Don't set(0) Buffererd Report Control Block [PurgeBuf] flag if [EntryID] setting fails during Report Control Block initialization [IEC61850-7-2:2010]</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 Ignore URCB Resv">
            <xs:documentation>[Resv] attribute value of the Unbuffered Report Control Block is Ignored(1) / Checked(0).</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="DSFlags">
    <xs:annotation>
      <xs:documentation>Dataset flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="0" caption="Bit 0 Leading 0 in dyndns name">
            <xs:documentation>Omit(1) / Add(0) leading 0 in dynamic dataset names with a number less than 10. This flag affects only dataset names created from Bdyndsname and Udyndsname attributes.</xs:documentation>
          </le:bit>
          <le:bit value="1" caption="Bit 1 Delete foreign DS">
            <xs:documentation>Don't delete(1) / Delete(0) dynamic datasets that are not created by us and are not required by any DI/AI. This setting applies only if IED supports Dynamic dataset creation.</xs:documentation>
          </le:bit>
          <le:bit value="8" caption="Bit 8 Init all RCBs">
            <xs:documentation>Initialize All(1) / Only required only(0) Report Blocks when station goes online. Only those Report Blocks with linked datasets that contain required DI/AIs (defined in XML) are initialized by default (0). All available Report Blocks are initialized if set to (1). This setting applies only if IED doesn't support Dynamic datasets.</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="WordType"/>
  </xs:simpleType>
  <xs:simpleType name="intgPdRange">
    <xs:annotation>
      <xs:documentation>Interval in milliseconds for generating periodic integrity reports [IEC61850-7-2:2010] Value 0 disables integrity reports (0...4294967294)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="Timer32bitType">
      <xs:maxInclusive value="4294967294"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DSrefString">
    <xs:annotation>
      <xs:documentation>Name of the dataset that must be enabled for this object. It is recommended not to use this attribute, unless you fully understand the implications. Any available dataset will be used if this attribute is not used. Sample value (IEDLD0/LLN0.mydataset) shown to help understand formatting of the attribute and is not valid.</xs:documentation>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:normalizedString">
          <xs:minLength value="1"/>
          <xs:maxLength value="129"/>
          <xs:pattern value="[A-Za-z][0-9A-Za-z_]{0,63}\/[A-Za-z][0-9A-Za-z_]{0,26}\.[A-Za-z][0-9A-Za-z_]*"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:normalizedString">
          <xs:minLength value="2"/>
          <xs:maxLength value="65"/>
          <xs:pattern value="@[A-Za-z][0-9A-Za-z_]*"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="DIdsFlags">
    <xs:annotation>
      <xs:documentation>DI object Dataset flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:mask value="0x03" caption="Buffered/Unbuffered dataset">
            <le:case value="0x00" caption="Buffered">
              <xs:documentation>Dataset that is linked to buffered report block is required for this DI</xs:documentation>
            </le:case>
            <le:case value="0x01" caption="Unbuffered">
              <xs:documentation>Dataset that is linked to unbuffered report block is required for this DI</xs:documentation>
            </le:case>
            <le:case value="0x02" caption="Any">
              <xs:documentation>Dataset that is linked to any report block can be used for this DI</xs:documentation>
            </le:case>
          </le:mask>
          <le:bit value="5" caption="Bit 5 Referenced DS only">
            <xs:documentation>Flag determines which datasets containing this DI object will be enabled (if there is more than 1 dataset that contains DI object). Flag can be used only if DSref="" attribute is specified. Use Any(0) / Referenced only(1) dataset for this DI object.</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="ByteType"/>
  </xs:simpleType>
  <xs:simpleType name="AIdsFlags">
    <xs:annotation>
      <xs:documentation>AI object Dataset flags (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:mask value="0x03" caption="Buffered/Unbuffered dataset">
            <le:case value="0x00" caption="Unbuffered">
              <xs:documentation>Dataset that is linked to unbuffered report block is required for this AI</xs:documentation>
            </le:case>
            <le:case value="0x01" caption="Buffered">
              <xs:documentation>Dataset that is linked to buffered report block is required for this AI</xs:documentation>
            </le:case>
            <le:case value="0x02" caption="Any">
              <xs:documentation>Dataset that is linked to any report block can be used for this AI</xs:documentation>
            </le:case>
          </le:mask>
          <le:bit value="5" caption="Bit 5 Referenced DS only">
            <xs:documentation>Flag determines which datasets containing this AI object will be enabled (if there is more than 1 dataset that contains AI object). Flag can be used only if DSref="" attribute is specified. Use Any(0) / Referenced only(1) dataset for this AI object.</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="ByteType"/>
  </xs:simpleType>
  <xs:simpleType name="ResponseTimeoutRange">
    <xs:annotation>
      <xs:documentation>Response timeout in seconds. Connection to IED will be closed if no repsponse to a sent messgage has been received within this timeout.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="Timer32bitType">
      <xs:minInclusive value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tLELDInst">
    <xs:annotation>
      <xs:documentation>Logical Device instance e.g. 'LD0' (max 64 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="tLDInst">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tLEPrefix">
    <xs:annotation>
      <xs:documentation>Prefix of the Logical Node name e.g. 'Q0' (0...11 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="tPrefix">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tLELNClassEnum">
    <xs:annotation>
      <xs:documentation>Logical Node Class e.g. 'XCBR' (4 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="tLNClassEnum">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tLELNInstOrEmpty">
    <xs:annotation>
      <xs:documentation>Logical Node Instance e.g. '1' (0...12 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="tLNInstOrEmpty">
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tLEExtensionFCEnum">
    <xs:restriction base="xs:Name">
      <xs:enumeration value="RP"/>
      <xs:enumeration value="BR"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tLEFCEnum">
    <xs:annotation>
      <xs:documentation>Functional Constraint e.g. 'ST' (2 characters)</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="tFCEnum tLEExtensionFCEnum"/>
  </xs:simpleType>
  <xs:simpleType name="tLETrgOps">
    <xs:annotation>
      <xs:documentation>Trigger Options (bit encoded)</xs:documentation>
      <xs:appinfo>
        <le:binaryEncode>
          <le:bit value="3" caption="Bit 3 [dupd]">
            <xs:documentation>Information Report that includes this data object Will(1) / Won't(0) be generated on Data-update [dupd] [IEC61850-7-2:2010]</xs:documentation>
          </le:bit>
          <le:bit value="4" caption="Bit 4 [integrity]">
            <xs:documentation>Information Report that includes this data object Will(1) / Won't(0) be generated periodicaly [integrity] upon [intgPd] timer expiration [IEC61850-7-2:2010]</xs:documentation>
          </le:bit>
        </le:binaryEncode>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="ByteType"/>
  </xs:simpleType>
  <xs:simpleType name="doNameType">
    <xs:annotation>
      <xs:documentation>Data object name. It may contain either single level name e.g. 'Pos' or multiple level name e.g. 'A.phsA' (1...64 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="tFullDOName">
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="daNameType">
    <xs:annotation>
      <xs:documentation>Data attribute name. It may contain either single level name e.g. 'stVal' or multiple level name e.g. 'cVal.mag.f' (1...64 characters)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="tFullAttributeName">
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="IEC61850DIOnValuesType">
    <xs:annotation>
      <xs:documentation>List of data values (for basic types e.g. {Enum}, {INT8}, etc) that will result in ON state when received from IED. Can be specified in decimal or hexadecimal notation separated by whitespaces or commas. (0x1234 0x02ef ...) or (12,0,458, ...)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="WordListType"/>
  </xs:simpleType>
  <xs:simpleType name="IEC61850DIOffValuesType">
    <xs:annotation>
      <xs:documentation>List of data values (for basic types e.g. {Enum}, {INT8}, etc) that will result in OFF state when received from IED. Can be specified in decimal or hexadecimal notation separated by whitespaces or commas. (0x1234 0x02ef ...) or (12,0,458, ...)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="WordListType"/>
  </xs:simpleType>
  <!--Basic type definitions-->
  <xs:simpleType name="AssocInteger">
    <xs:annotation>
      <xs:documentation>Integer used for AE qualifier and AP and AE invoke identifiers 0...99999</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt">
      <xs:maxInclusive value="99999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DatasetNameType">
    <xs:annotation>
      <xs:documentation>Dataset name begins with letter and can contain slash LD0/dynu</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:normalizedString">
      <xs:minLength value="1"/>
      <xs:maxLength value="97"/>
      <xs:pattern value="([A-Za-z][0-9A-Za-z_]{0,63}\/)?[A-Za-z][0-9A-Za-z_]{0,31}"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
